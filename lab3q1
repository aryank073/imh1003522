           ques--1
#include<bits/stdc++.h>
#define nl '\n'
using namespace std;

int main(){
    int n;
    cin >> n;
    int arr[n];
    for(int i=0;i<n;i++){
        cin >> arr[i];
    }
    int minIndex = INT_MAX;

    for (int i = 0; i < n; ++i) {
        for (int j = i + 1; j < n; ++j) {
            if (arr[i] == arr[j]) {
                minIndex =min(minIndex, i);
                break;
            }
        }
    }
    if(minIndex==INT_MAX){
        cout << "no repeating elements" << nl;
    }
    else{
        cout << "repeating index is:" << minIndex << nl;
    }
}





        ques--2
#include<bits/stdc++.h>
#define nl '\n'
using namespace std;

int main(){
    int n;
    cin >> n;
    int arr[n];
    for(int i=0;i<n;i++){
        cin >> arr[i];
    }
    int max=arr[0];
    for(int i=1;i<n;i++){
        if(arr[i]>max){
            max=arr[i];
        }
        else{
            break;
        }
    }

    cout << "max element is:" << max << nl;
    
}
