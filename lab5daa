#include<bits/stdc++.h>
using namespace std;
int main(){
    int n;
    cin >> n;
    vector<int> arr(n);
    for(int i=0;i<n;i++){
        cin >> arr[i];
    }
    while(!arr.empty()){
        int min=arr[0];
        for(int i=1;i<arr.size();i++){
            if(arr[i]<min){
                min=arr[i];
            }
        }
    
    int newsize=0;
    for(int i=0;i<arr.size();i++){
        arr[i]-=min;
        if(arr[i]>0){
            arr[newsize++]=arr[i];
        }
    }
    arr.resize(newsize);
    cout << newsize << endl;
    }
    return 0;
}
